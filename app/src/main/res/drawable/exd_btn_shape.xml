<?xml version="1.0" encoding="utf-8"?>
<inset xmlns:android="http://schemas.android.com/apk/res/android"
  android:insetBottom="@dimen/exd_button_inset_vertical"
  android:insetLeft="@dimen/exd_button_inset_horizontal"
  android:insetRight="@dimen/exd_button_inset_horizontal"
  android:insetTop="@dimen/exd_button_inset_vertical">
  <shape android:shape="rectangle">
    <corners android:radius="@dimen/exd_action_corner_radius" />
    <solid android:color="?android:colorButtonNormal" />
    <padding
      android:bottom="@dimen/exd_button_padding_vertical"
      android:left="@dimen/exd_button_padding_horizontal"
      android:right="@dimen/exd_button_padding_horizontal"
      android:top="@dimen/exd_button_padding_vertical" />
  </shape>
</inset>